{{>partials/header}}

    <div class="container col-lg-10 text-center mt-3">

        <div class="row">

            <h4 class="mt-3 mb-5">
                <span class="fw-bold text-warning"><i class="bi bi-award"></i>
                SUBSCRIPTION_TRANSACITON_ID:</span> <span class="fw-bold text-success">{{subsTransaction.transaction_id}}</span>
            </h4>

            <div class="col-lg-6">
                {{#subsTransaction}}
                    <ul class="list-group list-group-flush text-start">
                        <li class="list-group-item"><b>PLAN_NAME:</b> {{plan.name}}</li>
                        <li class="list-group-item"><b>CURRENT_PERIOD_START:</b> {{plan.current_period_start}}</li>
                        <li class="list-group-item"><b>CURRENT_PERIOD_END:</b> {{plan.current_period_end}}</li>
                        <li class="list-group-item"><b>STATUS:</b> {{status}}</li>

                        <li class="list-group-item"><b>CREATED:</b> {{created_at}}</li>
                    </ul>
                {{/subsTransaction}}
            </div>

            <div class="col-lg-6">
                {{#subsTransaction}}
                    <ul class="list-group list-group-flush text-start">
                        <li class="list-group-item"><b>PAYMENT_METHOD:</b> {{payment_method.card_id}}</li>
                        <li class="list-group-item"><b>CARD_BRAND:</b> {{payment_method.brand}}</li>
                        <li class="list-group-item"><b>CARD_EXP_MONTH:</b> {{payment_method.exp_month}}</li>
                        <li class="list-group-item"><b>CARD_EXP_YEAR:</b> {{payment_method.exp_year}}</li>
                        <li class="list-group-item"><b>CARD_LAST4:</b> {{payment_method.last4}}</li>

                    </ul>
                {{/subsTransaction}}
            </div>

        </div>

    </div>

{{>partials/footer}}
